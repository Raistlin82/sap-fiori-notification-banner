{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/sap/public/bc/icf/logoff",
    "logoutPage": "/index.html"
  },
  "routes": [
    {
      "source": "^/sap/bc/rest/(.*)$",
      "target": "/sap/bc/rest/$1",
      "destination": "backend",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/sap/(.*)$",
      "target": "/sap/$1",
      "destination": "backend",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa"
    }
  ]
}